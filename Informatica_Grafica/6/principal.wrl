#Routes
#	Conectamos el momento en el que tocamos el TouchSensor con el momento en el que comenzará el TimeSensor
ROUTE TSensor1.touchTime TO Time1.startTime
#	Conectamos cada instante que envía el TimeSensor con el interpolador
ROUTE Time1.fraction_changed TO AceleracionInterp.set_fraction
#	Conectamos cada nuevo valor de salida del interpolador con el objeto a animar
ROUTE AceleracionInterp.value_changed TO Esqueleto.set_translation
#	Conectamos cada nuevo valor de salida del interpolador con el objeto a animar (rotacion)
ROUTE Time1.fraction_changed TO RotationEsqueleto.set_fraction
#	Conectamos cada nuevo valor de salida del interpolador con el objeto a animar (rotacion)
ROUTE RotationEsqueleto.value_changed TO Esqueleto.set_rotation


#movimiento de las pierna1
ROUTE TSensor1.touchTime TO Time2.startTime
ROUTE Time2.fraction_changed TO Rotationjoint1.set_fraction
ROUTE Rotationjoint1.value_changed TO pierna1.set_rotation

#movimiento de las pierna2
ROUTE TSensor1.touchTime TO Time2.startTime
ROUTE Time2.fraction_changed TO Rotationjoint2.set_fraction
ROUTE Rotationjoint2.value_changed TO pierna2.set_rotation

